<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>Authenticate</servlet-name>
        <servlet-class>com.humber.controller.Authenticate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterClass</servlet-name>
        <servlet-class>com.humber.controller.RegisterClass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Membership</servlet-name>
        <servlet-class>com.humber.controller.Membership</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Authenticate</servlet-name>
        <url-pattern>/Authenticate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterClass</servlet-name>
        <url-pattern>/RegisterClass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Membership</servlet-name>
        <url-pattern>/Membership</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
